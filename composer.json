{
    "name": "samsonasik/force-https-module",
    "type": "library",
    "description": "Force Https Module for Laminas Mvc and Mezzio application",
    "keywords": [
        "laminas3",
        "expressive",
        "middleware",
        "force",
        "https",
        "http",
        "psr7",
        "psr11",
        "psr-15"
    ],
    "homepage": "https://github.com/samsonasik/ForceHttpsModule",
    "license": "MIT",
    "authors": [
        {
            "name": "Abdul Malik Ikhsan",
            "email": "samsonasik@gmail.com",
            "homepage": "http://samsonasik.wordpress.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "webmozart/assert": "^1.11 || ^2.1"
    },
    "conflict": {
        "mezzio/mezzio": "<3.0",
        "laminas/laminas-mvc": "<3.0"
    },
    "require-dev": {
        "kahlan/kahlan": "^6.0",
        "laminas/laminas-coding-standard": "^3.0",
        "laminas/laminas-mvc": "^3.8",
        "mezzio/mezzio": "^3.20.1",
        "php-coveralls/php-coveralls": "^2.7",
        "phpstan/phpstan": "^2.0.4",
        "phpstan/phpstan-webmozart-assert": "^2.0",
        "rector/rector": "dev-main"
    },
    "config": {
        "bin-dir": "bin",
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "extra": {
        "laminas": {
            "module": "ForceHttpsModule"
        }
    },
    "autoload": {
        "psr-4": {
            "ForceHttpsModule\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ForceHttpsModule\\Spec\\": "spec/"
        }
    },
    "scripts": {
        "cs-check": "phpcs",
        "cs-fix": "phpcbf"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
